{
  "name": "@sabertazimi/ojs",
  "private": true,
  "version": "2.3.0",
  "description": "Archive for OJ code",
  "main": "index.ts",
  "scripts": {
    "format": "prettier --write leetCode/**/*.ts",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --ext .ts ./leetCode ./codeWars",
    "lint:type-check": "tsc --noEmit",
    "prelease": "standard-version --dry-run -s",
    "release": "standard-version -s",
    "start": "yarn test:watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/OJs.git"
  },
  "keywords": [
    "algorithm",
    "algorithms",
    "OJ",
    "OJs",
    "leetcode",
    "leetcode-solutions",
    "leetcode-cpp",
    "leetcode-typescript",
    "typescript"
  ],
  "author": "sabertazimi <sabertazimi@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/OJs/issues"
  },
  "homepage": "https://github.com/sabertazimi/OJs#readme",
  "packageManager": "yarn@3.1.1",
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.6.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.17",
    "ci-info": "^3.3.0",
    "consola": "^2.15.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
