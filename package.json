{
  "name": "@sabertazimi/ojs",
  "private": true,
  "version": "2.2.0",
  "description": "Archive for OJ code",
  "main": "index.ts",
  "scripts": {
    "format": "prettier --write leetCode/**/*.ts",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --fix --ext .ts ./leetCode",
    "lint:type-check": "tsc --noEmit",
    "prelease": "standard-version --dry-run -s",
    "release": "standard-version -s",
    "start": "yarn test:watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/OJs.git"
  },
  "keywords": [
    "algorithm",
    "algorithms",
    "OJ",
    "OJs",
    "leetcode",
    "leetcode-solutions",
    "leetcode-cpp",
    "leetcode-typescript",
    "typescript"
  ],
  "author": "sabertazimi <sabertazimi@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/OJs/issues"
  },
  "homepage": "https://github.com/sabertazimi/OJs#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "ci-info": "^3.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.1",
    "prettier": "^2.4.0",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  }
}
