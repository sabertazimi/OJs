{
  "name": "@sabertazimi/ojs",
  "private": true,
  "version": "2.4.0",
  "description": "Archive for OJ code",
  "main": "index.ts",
  "scripts": {
    "format": "prettier --write leetCode/**/*.ts",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --ext .ts ./leetCode ./codeWars",
    "lint:type-check": "tsc --noEmit",
    "changeset": "commit-and-tag-version --dry-run -s",
    "release": "commit-and-tag-version -s",
    "start": "yarn test:watch",
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/OJs.git"
  },
  "keywords": [
    "algorithm",
    "algorithms",
    "OJ",
    "OJs",
    "leetcode",
    "leetcode-solutions",
    "leetcode-cpp",
    "leetcode-typescript",
    "typescript"
  ],
  "author": "sabertazimi <sabertazimi@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/OJs/issues"
  },
  "homepage": "https://github.com/sabertazimi/OJs#readme",
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.13.3",
    "@types/node": "^18.14.6",
    "@vitest/coverage-c8": "^0.29.2",
    "c8": "^7.13.0",
    "ci-info": "^3.8.0",
    "commit-and-tag-version": "^11.1.0",
    "consola": "^2.15.3",
    "eslint": "^8.35.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
  }
}
